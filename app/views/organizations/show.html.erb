<div class="row">
  <div class="col s10">
    <h3><%= @organization.name %></h3>
  </div>
  <div class="col s2">
    <%= link_to 'Edit', edit_organization_path(@organization), class: 'btn-flat right waves-effect waves-teal' %>
  </div>
</div>

<p>
  <strong>Name:</strong>
  <%= @organization.name %>
</p>

<table>
  <tr>
    <th>Admins</th>
  </tr>

  <% @admins.each do |user| %>
      <tr>
        <td>
          <%= user.email %>
        </td>
      </tr>
  <% end %>
</table>

