<div class="page-content">
  <div class="row">
    <div class="col s10">
      <h3><%= @organization.name %></h3>
    </div>
    <div class="col s2">
      <%= link_to 'Edit', edit_organization_path(@organization), class: 'btn-flat right waves-effect waves-teal' %>
    </div>
  </div>

  <p>
    <strong>Name:</strong>
    <%= @organization.name %>
  </p>
</div>
<div class="page-content">
  <ul class="tabs">
    <li class="tab"><a href="#teams">Teams</a></li>
    <li class="tab"><a href="#admins">Admins</a></li>
    <li class="tab"><a href="#members">Members</a></li>
  </ul>
  <div id="teams">
    <table>
      <% @organization.teams.each do |team| %>
          <tr>
            <td>
              <%= link_to team.name, team %>
            </td>
          </tr>
      <% end %>
    </table>
  </div>
  <div id="admins">
    <table>
      <% @admins.each do |user| %>
          <tr>
            <td>
              <%= user.email %>
            </td>
          </tr>
      <% end %>
    </table>
  </div>
  <div id="members">Test 3
  </div>

</div>
